{
  "name": "nuxt3-starter",
  "version": "1.0.1",
  "description": "A Nuxt 3 template and boilerplate with a lot of useful features. Nuxt 3 + Tailwindcss + Nuxt Layer",
  "author": "Renato Nabinger",
  "private": false,
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git://github.com/renatoxm/nuxt-starter"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "lint:fix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "release": "release-it"
  },
  "dependencies": {
    "@nuxt/content": "^2.12.0",
    "@nuxt/devtools": "^1.0.8",
    "@nuxtjs/color-mode": "^3.3.2",
    "@nuxtjs/tailwindcss": "^6.11.4",
    "@pinia/nuxt": "^0.5.1",
    "@tailwindcss/typography": "^0.5.10",
    "@vueuse/core": "^10.7.2",
    "@vueuse/nuxt": "^10.7.2",
    "axios": "^1.6.7",
    "nuxt": "^3.10.2",
    "nuxt-headlessui": "^1.1.5",
    "nuxt-icon": "^0.6.8",
    "pinia": "^2.1.7",
    "serve": "^14.2.1",
    "stringify": "^5.2.0",
    "tailwindcss": "^3.4.1",
    "unplugin-vue-components": "^0.26.0",
    "vue": "^3.4.19",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@nuxtjs/i18n": "^8.1.1",
    "consola": "^3.2.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "release-it": "^17.0.5",
    "sass": "^1.71.0",
    "typescript": "^5.3.3",
    "vue-tsc": "^1.8.27"
  },
  "release-it": {
    "github": {
      "release": true,
      "releaseName": "v${version}"
    },
    "git": {
      "commitMessage": "chore(release): release v${version}"
    }
  }
}
